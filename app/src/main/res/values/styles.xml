<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Yes" parent="Theme.AppCompat.Light">
        <item name="colorControlHighlight">@color/cyan_A700</item>
        <item name="colorButtonNormal">@color/cyan_A400</item>
    </style>

    <style name="Skip" parent="Theme.AppCompat.Light">
        <item name="colorControlHighlight">@color/grey_500</item>
        <item name="colorButtonNormal">@color/blue_grey_200</item>
    </style>

    <style name="Skip1" parent="Theme.AppCompat.Light">
        <item name="colorControlHighlight">@color/deep_purple_A100</item>
        <item name="colorButtonNormal">@color/deep_purple_A700</item>
    </style>


</resources>
